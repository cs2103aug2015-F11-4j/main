<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.ListView?>

<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<TableView fx:id="viewAllTable">
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
			<columns>
				<TableColumn fx:id="eventIdColumn" text="ID" />
				<TableColumn fx:id="eventTitleColumn" text="Title" />
				<TableColumn fx:id="eventDateColumn" text="Date" />
				<TableColumn fx:id="eventTimeColumn" text="Time" />
			</columns>
		</TableView>
	</children>
</fx:root>