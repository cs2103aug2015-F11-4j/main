<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.CheckBox?>

<fx:root type="StackPane" xmlns="http://javafx.com/javafx/8.0.40"
	xmlns:fx="http://javafx.com/fxml">
	<children>
		<GridPane fx:id="datedEventGridPane" xmlns="http://javafx.com/javafx/8.0.40"
			xmlns:fx="http://javafx.com/fxml" hgap="10" vgap="10" prefWidth="1000.0"
			prefHeight="50.0" maxWidth="1000.0" maxHeight="50.0">
			<columnConstraints>
				<ColumnConstraints percentWidth="5" />
				<ColumnConstraints percentWidth="56" />
				<ColumnConstraints percentWidth="35" />
				<ColumnConstraints percentWidth="4" />
			</columnConstraints>
			<children>
				<Label fx:id="lblDatedEventID" GridPane.rowIndex="0"
					GridPane.columnIndex="0" wrapText="true" />
				<Label fx:id="lblDatedEventTitle" GridPane.rowIndex="0"
					GridPane.columnIndex="1" wrapText="true" />
				<Label fx:id="lblDatedEventDate" GridPane.rowIndex="0"
					GridPane.columnIndex="2" wrapText="true" GridPane.halignment="CENTER" />
				<CheckBox fx:id="checkBoxDone" GridPane.rowIndex="0" 
					GridPane.columnIndex="3" />
			</children>
		</GridPane>
	</children>
</fx:root>